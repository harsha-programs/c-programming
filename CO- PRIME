#include<stdio.h>
int main () {
    int a , b, temp ;
    printf("enter two numbers a and b :");
    scanf("%d %d" , &a , &b);   

 for (int i=1; i<=a && i<=b; i++)
    {
        if (a % i == 0 && b % i == 0)
        {
         int GCD = i;
        }
    }
  printf("the GCD of a and b is %d\n", temp);

  if (a == 1)
  {
    printf("a and b are co-primes");
  }
  else
  {
    printf("a and b are not co-primes");
  } 
}
